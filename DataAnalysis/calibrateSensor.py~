import numpy as np
calibrationData = {}
adc4avgs = []
adc0avgs = []
for line in fileName:
    line = eval(line)
    adc4 = []
    adc0 = []
    sensorId = line['source_addr']
    if sensorId not in calibrationData.keys():
        calibrationData[sensorId] = 0

    for sample in line['samples']:
        adc4.append(sample['adc-4'])
        adc0.append(sample['adc-0'])
    adc4avgs.append(np.mean(adc4))
    adc0avgs.append(np.mean(adc0))


print calibrationData;
